      </intent>
      <intent>
        <action android:name="android.intent.action.VIEW" />
        <data android:scheme="lyft" />
      </intent>
      <intent>
        <action android:name="android.intent.action.VIEW" />
        <data android:scheme="transit" />
      </intent>
      <intent>
        <action android:name="android.intent.action.VIEW" />
